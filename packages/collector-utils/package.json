{
  "name": "@zerobias-org/collector-utils",
  "version": "1.0.11",
  "description": "Common utilities for collector bots - HTTP fetching, data chunking, CSV/YAML parsing",
  "type": "module",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js",
      "default": "./dist/src/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf dist/",
    "transpile": "tsc -b",
    "build": "npm run lint && npm run transpile",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint --fix 'src/**/*.ts'",
    "test": "mocha --inline-diffs --reporter=list test/unit/**/*.ts",
    "test:watch": "npm run test -- --watch --reporter=min",
    "nx:prepublish": "prepublish-standalone",
    "nx:publish": "publish"
  },
  "files": [
    "dist/src",
    "CHANGELOG.md"
  ],
  "keywords": [
    "collector",
    "utils",
    "etl",
    "batch"
  ],
  "author": "ZeroBias <support@zerobias.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/zerobias-org/util",
    "directory": "packages/collector-utils"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "dependencies": {
    "@zerobias-org/logger": "^2.0.4",
    "@zerobias-org/util-connector": "^1.0.2"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  }
}
