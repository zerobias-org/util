{
  "entryPoints": ["src/index.ts"],
  "out": "docs/api",
  "exclude": ["**/node_modules/**", "**/test/**", "**/dist/**"],
  "excludePrivate": true,
  "excludeProtected": false,
  "includeVersion": true,
  "readme": "README.md",
  "plugin": [],
  "sort": ["source-order"],
  "treatWarningsAsErrors": false,
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": false
  }
}
