{
  "name": "@zerobias-org/logger",
  "version": "2.0.18",
  "description": "ZeroBias Logger - standard logging for all services",
  "type": "module",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "browser": {
    "./dist/src/index.js": "./dist/src/browser.js"
  },
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "browser": "./dist/src/browser.js",
      "import": "./dist/src/index.js",
      "default": "./dist/src/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf dist/",
    "transpile": "tsc -b",
    "build": "npm run lint && npm run transpile",
    "nx:build": "npm run transpile",
    "lint": "eslint 'src/**/*.ts' 'test/**/*.ts'",
    "lint:fix": "eslint --fix 'src/**/*.ts' 'test/**/*.ts'",
    "test": "npm run test:unit",
    "test:unit": "node --import tsx/esm ../../node_modules/mocha/bin/mocha.js --exit --extensions ts --inline-diffs --reporter=list test/unit/**/*.ts"
  },
  "files": [
    "dist/src",
    "CHANGELOG.md"
  ],
  "keywords": [
    "logger",
    "winston"
  ],
  "author": "ZeroBias <support@zerobias.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/zerobias-org/util",
    "directory": "packages/logger"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  }
}
