{
  "name": "@zerobias-org/util-hub-module-utils",
  "description": "Hub Module utils",
  "version": "2.0.1",
  "type": "module",
  "main": "dist/src/index.js",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    }
  },
  "scripts": {
    "clean": "rm -rf dist/ generated/",
    "transpile": "tsc",
    "build": "npm run generate && npm run transpile",
    "generate": "hub-generator generate -g hub-module -i test/unit/enum.yml -o generated/",
    "test": "mocha --inline-diffs --reporter=list test/unit/**/*.ts",
    "test:watch": "npm run test:generate && npm run test -- --watch --reporter=min",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zerobias-org/util.git",
    "directory": "hub-module-utils"
  },
  "author": "ctamas@zerobias.com",
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "dist",
    "bom.json"
  ],
  "dependencies": {
    "@zerobias-org/types-amazon-js": "^1.0.0",
    "@zerobias-org/types-core": "^1.0.0",
    "@zerobias-org/types-core-js": "^1.0.0",
    "@zerobias-org/types-microsoft-js": "^1.0.0",
    "change-case": "^5.0.0",
    "jsonata": "^2.0.0"
  },
  "devDependencies": {
    "@zerobias-org/util-codegen": "^2.0.4"
  }
}
