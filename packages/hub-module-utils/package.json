{
  "name": "@zerobias-org/util-hub-module-utils",
  "description": "Hub Module utils",
  "version": "2.0.0",
  "type": "module",
  "main": "dist/src/index.js",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    }
  },
  "scripts": {
    "clean": "rm -rf dist/ generated/",
    "transpile": "tsc",
    "build": "npm run generate && npm run transpile",
    "generate": "hub-generator generate -g hub-module -i test/unit/enum.yml -o generated/",
    "test": "mocha --inline-diffs --reporter=list test/unit/**/*.ts",
    "test:watch": "npm run test:generate && npm run test -- --watch --reporter=min",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zerobias-org/util.git",
    "directory": "hub-module-utils"
  },
  "author": "ctamas@zerobias.com",
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "dist",
    "bom.json"
  ],
  "devDependencies": {
    "@types/chai": "latest",
    "@types/mocha": "latest",
    "@types/node": "latest",
    "@types/request": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "@zerobias-org/eslint-config": "latest",
    "@zerobias-org/util-codegen": "latest",
    "@zerobias-org/util-connector": "latest",
    "chai": "latest",
    "eslint": "latest",
    "eslint-config-airbnb-base": "latest",
    "eslint-config-airbnb-typescript": "latest",
    "eslint-plugin-import": "latest",
    "mocha": "latest",
    "ts-node": "latest",
    "typescript": "latest"
  },
  "peerDependencies": {
    "@zerobias-org/types-amazon-js": "latest",
    "@zerobias-org/types-core": "latest",
    "@zerobias-org/types-core-js": "latest",
    "@zerobias-org/types-microsoft-js": "latest"
  },
  "dependencies": {
    "@typescript-eslint/typescript-estree": "^7.18.0",
    "camel-case": "latest",
    "jsonata": "latest",
    "pascal-case": "latest",
    "snake-case": "latest"
  },
