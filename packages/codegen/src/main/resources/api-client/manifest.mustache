{
  "serviceName": "{{apiName}}",
  "version": "{{version}}",
  "codegenVersion": "{{codegenVersion}}",
  "generatedAt": "{{generatedDate}}",
  "clientClass": "{{apiName}}Client",
  "factoryFunction": "new{{apiName}}",
  "operations": [
{{#apiInfo}}
{{#apis}}
{{#operations}}
{{#operation}}
    {
      "operationId": "{{operationId}}",
