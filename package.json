{
  "name": "@zerobias-org/util",
  "version": "1.0.0",
  "description": "Utility repository for ZeroBias modules",
  "type": "module",
  "private": true,
  "workspaces": [
    "packages/*",
    "packages/invoker/*"
  ],
  "scripts": {
    "build": "nx affected -t nx:build",
    "build:full": "nx run-many -t nx:build",
    "clean": "nx affected -t clean && nx reset && find . -name tsconfig.tsbuildinfo -delete || echo \"\"",
    "clean:full": "nx run-many -t clean && nx reset && find . -name tsconfig.tsbuildinfo -delete || echo \"\"",
    "docs": "nx run-many -t docs",
    "generate:bom": "nx run-many -t generate:bom",
    "lerna:publish": "lerna publish --conventional-commits",
    "lerna:publish-from-package": "lerna publish from-package",
    "lerna:dry-run": "lerna version --no-git-tag-version --no-push --conventional-commits",
    "lerna:version": "lerna version --no-git-tag-version --no-push --conventional-commits --yes",
    "lerna:version:hotfix": "lerna version --no-git-tag-version --no-push --conventional-prerelease --preid hotfix --conventional-commits --yes",
    "lint": "nx affected -t lint",
    "lint:full": "nx run-many -t lint",
    "lint:fix": "nx run-many -t lint:fix",
    "reset": "npm run clean:full && npm install && npm run build:full",
    "test": "nx affected -t test",
    "test:full": "nx run-many -t test"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:zerobias-org/util.git"
  },
  "author": "team@zerobias.com",
  "license": "ISC",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "overrides": {
    "@zerobias-org/types-core-js": "$@zerobias-org/types-core-js",
    "mocha": {
      "diff": "^8.0.3"
    },
    "lodash": "^4.17.23",
    "tar": "^7.5.7",
    "@stoplight/spectral-cli": {
      "magic-string": "^0.30.0"
    },
    "glob": ">=13.0.0",
    "fast-xml-parser": ">=5.3.4"
  },
  "dependencies": {
    "@stoplight/json-ref-resolver": "^3.1.6",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@zerobias-com/platform-sdk": "^1.1.3",
    "@zerobias-org/module-interface-dataproducer-hub-sdk": "^1.2.32",
    "@zerobias-org/types-amazon-js": "^1.0.33",
    "@zerobias-org/types-core": "^1.0.20",
    "@zerobias-org/types-core-js": "^1.2.15",
    "@zerobias-org/types-format-spectral-ruleset": "^1.0.35",
    "@zerobias-org/types-microsoft-js": "^1.0.32",
    "axios": "^1.13.2",
    "chalk": "^5.6.2",
    "change-case": "^5.0.0",
    "csv-parse": "^6.0.0",
    "dockerode": "^4.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-unicorn": "^62.0.0",
    "form-data": "^4.0.0",
    "get-port": "^7.1.0",
    "js-yaml": "^4.0.0",
    "jsonata": "^2.0.0",
    "qs": "^6.14.0",
    "uuid": "^13.0.0",
    "winston": "^3.2.0",
    "winston-transport": "^4.9.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@stoplight/spectral-cli": "^6.15.0",
    "@types/chai": "^5.2.3",
    "@types/dockerode": "^3.3.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.0.0",
    "@types/qs": "^6.9.18",
    "@types/request": "latest",
    "@zerobias-org/devops-tools": "^1.0.50",
    "@zerobias-org/eslint-config": "^1.0.30",
    "chai": "^6.0.0",
    "husky": "^9.1.7",
    "lerna": "^9.0.0",
    "mocha": "^11.7.5",
    "nock": "^14.0.10",
    "nx": "^22.0.0",
    "rimraf": "^6.1.0",
    "tsx": "^4.21.0",
    "typedoc": "^0.28.15",
    "typescript": "^5.9.3"
  }
}
